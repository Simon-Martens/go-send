<!DOCTYPE html>
<html lang="{{.Locale}}">


<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>Send</title>
	<base href="/">
	<meta name="robots" content="all,noarchive">
	<meta name="google" content="nositelinkssearchbox">
	<meta name="description"
		content="Encrypt and send files with a link that automatically expires to ensure your important documents don't stay online forever.">
	<meta name="theme-color" content="#220033">
	<meta name="msapplication-TileColor" content="#220033">
	<link rel="stylesheet" href="/inter.css">
	<style {{.NonceAttr}}>
		:root {
			--color-primary: {{ .Theme.Primary }};
			--color-primary-accent: {{ .Theme.Accent }};
		}
	</style>
	{{if .Assets.CSS}}
	<link rel="stylesheet" href="{{ assetURL .Assets.CSS }}">{{end}}
	<!-- <link rel="manifest" href="/app.webmanifest"> -->
	{{if .Assets.AppleTouchIcon}}
	<link rel="apple-touch-icon" sizes="180x180" href="{{ assetURL .Assets.AppleTouchIcon }}">{{end}}
	{{if .Assets.Favicon32}}
	<link rel="icon" type="image/png" sizes="32x32" href="{{ assetURL .Assets.Favicon32 }}">{{end}}
	{{if .Assets.Favicon16}}
	<link rel="icon" type="image/png" sizes="16x16" href="{{ assetURL .Assets.Favicon16 }}">{{end}}
	{{if .Assets.SafariPinnedTab}}
	<link rel="mask-icon" href="{{ assetURL .Assets.SafariPinnedTab }}" color="#838383">{{end}}
	<link rel="icon" href="/favicon.ico">
	{{if .Assets.JS}}
	<script type="module" defer src="{{ assetURL .Assets.JS }}"></script>{{end}}
</head>

<body class="flex flex-col items-center font-sans md:h-screen md:bg-grey-10 dark:bg-black">
	<noscript>
		<div class="noscript">
			<h2>JavaScript Required</h2>
			<p>Firefox Send requires JavaScript to encrypt your files.</p>
		</div>
	</noscript>
	<main></main>
	{{template "footer" .Footer}}
</body>
<script {{.NonceAttr}}>
	var LIMITS = {{toJSON .State.Limits}};
	var WEB_UI = {{toJSON .State.WebUI}};
	var DEFAULTS = {{toJSON .State.Defaults}};
	var PREFS = {};
	var downloadMetadata = {{rawJSON .State.DownloadMetadata}};
</script>

</html>
